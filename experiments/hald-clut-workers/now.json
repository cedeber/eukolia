{
  "version": 2,
  "name": "pimio",
  "regions": ["bru"],
  "alias": "pimio.cedeber.fr",
  "builds": [
    { "src": "next.config.js", "use": "@now/next" },
    { "src": "static", "use": "@now/static" }
  ],
  "routes": [
    { "src": "/service-worker.js", "dest": "/static/workers/service-worker.js" },
    { "src": "/manifest.webmanifest", "dest": "/static/manifest.webmanifest" }
  ]
}
