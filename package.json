{
  "name": "web-client-boilerplate",
  "private": true,
  "version": "1.0.0",
  "description": "Web client boilerplate",
  "scripts": {
    "clean": "rimraf .cache dist",
    "dev": "parcel src/index.html",
    "test": "ts-jest",
    "build": "parcel build src/index.html --no-source-maps --no-cache --log-level 4 --detailed-report 30"
  },
  "browserslist": [
    "last 3 years and > 0.2%",
    "> 0.5%",
    "Firefox ESR",
    "not dead"
  ],
  "alias": {
    "react": "preact/compat",
    "react-dom": "preact/compat"
  },
  "dependencies": {
    "classnames": "^2.2.6",
    "graphql": "^14.4.2",
    "preact": "^10.0.0-rc.1",
    "react-redux": "^7.1.0",
    "redux": "^4.0.4",
    "wouter": "^2.1.1"
  },
  "devDependencies": {
    "@babel/core": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "@hint/configuration-development": "^7.0.3",
    "@types/classnames": "^2.2.9",
    "@types/jest": "^24.0.17",
    "@types/react": "^16.8.25",
    "@types/react-dom": "^16.8.4",
    "@types/react-redux": "^7.1.1",
    "@types/react-router": "^5.0.3",
    "@types/react-router-dom": "^4.3.4",
    "@types/wouter": "^2.0.1",
    "@typescript-eslint/eslint-plugin": "^1.12.0",
    "@typescript-eslint/parser": "^1.12.0",
    "autoprefixer": "^9.6.1",
    "core-js": "^2.6.9",
    "eslint": "^5.16.0",
    "eslint-config-prettier": "^4.3.0",
    "eslint-plugin-compat": "^3.3.0",
    "eslint-plugin-graphql": "^3.0.3",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.14.2",
    "eslint-plugin-react-hooks": "^1.6.1",
    "hint": "^5.2.0",
    "jest": "^24.8.0",
    "parcel-bundler": "^1.12.3",
    "postcss-modules": "^1.4.1",
    "postcss-preset-env": "^6.7.0",
    "prettier": "^1.18.2",
    "rimraf": "^2.6.3",
    "sass": "^1.22.9",
    "stylelint": "^10.1.0",
    "stylelint-config-prettier": "^5.2.0",
    "stylelint-config-recommended-scss": "^3.3.0",
    "stylelint-config-standard": "^18.3.0",
    "stylelint-prettier": "^1.1.1",
    "stylelint-scss": "^3.9.3",
    "typescript": "^3.5.3"
  }
}
