{
  "name": "web-client-boilerplate",
  "private": true,
  "version": "1.0.0",
  "description": "Web client boilerplate",
  "scripts": {
    "clean": "rimraf .cache .parcel-cache dist",
    "dev": "parcel my_project/index.html",
    "test": "ts-jest",
    "build": "parcel build my_project/index.html --no-source-maps --no-cache --log-level 4 --detailed-report 30"
  },
  "browserslist": [
    "last 3 years and > 0.2%",
    "> 0.5%",
    "Firefox ESR",
    "not dead",
    "not IE <= 11",
    "not OperaMini all"
  ],
  "alias": {
    "react": "preact/compat",
    "react-dom": "preact/compat"
  },
  "dependencies": {
    "@apollo/react-hooks": "^3.0.1",
    "@blueprintjs/core": "^3.18.0",
    "@blueprintjs/icons": "^3.10.0",
    "apollo-cache-inmemory": "^1.6.3",
    "apollo-client": "^2.6.4",
    "apollo-link-http": "^1.5.15",
    "apollo-utilities": "^1.3.2",
    "classnames": "^2.2.6",
    "graphql": "^14.5.4",
    "graphql-tag": "^2.10.1",
    "micro": "^9.3.5-canary.3",
    "normalize.css": "^8.0.1",
    "preact": "^10.0.0-rc.1",
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "serve-handler": "^6.1.1",
    "use-subscription": "^1.0.0",
    "wouter": "^2.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "@types/classnames": "^2.2.9",
    "@types/jest": "^24.0.17",
    "@types/react": "^16.9.2",
    "@types/react-dom": "^16.8.5",
    "@typescript-eslint/eslint-plugin": "^2.0.0",
    "@typescript-eslint/parser": "^2.0.0",
    "autoprefixer": "^9.6.1",
    "core-js": "^2.6.9",
    "eslint": "^6.1.0",
    "eslint-config-prettier": "^6.0.0",
    "eslint-plugin-compat": "^3.3.0",
    "eslint-plugin-graphql": "^3.0.3",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.14.2",
    "eslint-plugin-react-hooks": "^2.3.0",
    "jest": "^24.9.0",
    "parcel": "^1.12.3",
    "postcss-modules": "^1.4.1",
    "postcss-preset-env": "^6.7.0",
    "prettier": "^1.18.2",
    "rimraf": "^3.0.0",
    "sass": "^1.22.10",
    "stylelint": "^12.0.0",
    "stylelint-config-prettier": "^6.0.0",
    "stylelint-config-recommended-scss": "^4.1.0",
    "stylelint-config-standard": "^19.0.0",
    "stylelint-prettier": "^1.1.1",
    "stylelint-scss": "^3.9.3",
    "typescript": "^3.5.3"
  }
}
