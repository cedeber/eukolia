{
  "name": "typescript-rollup",
  "version": "1.0.0",
  "description": "...",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "tsc:prod": "tsc --project tsconfig.json",
    "tsc:watch": "tsc -w --project tsconfig.json",
    "rollup:prod": "rollup --config rollup.config.js --environment INCLUDE_DEPS,BUILD:production,NODE_ENV:production",
    "rollup:dev": "npm run rollup:prod -- --environment INCLUDE_DEPS,BUILD:development,NODE_ENV:development",
    "rollup:watch": "npm run rollup:prod -- --environment INCLUDE_DEPS,BUILD:development,NODE_ENV:development --watch",
    "build:dev": "npm run tsc:prod && npm run rollup:dev",
    "build:prod": "npm run tsc:prod && npm run rollup:prod"
  },
  "dependencies": {
    "@emotion/core": "^10.0.7",
    "@emotion/styled": "^10.0.7",
    "@types/react": "^16.8.6",
    "@types/react-dom": "^16.8.2",
    "react": "^16.8.3",
    "react-dom": "^16.8.3",
    "rollup": "^1.4.1",
    "rollup-plugin-commonjs": "^9.2.1",
    "rollup-plugin-node-resolve": "^4.0.1",
    "rollup-plugin-replace": "^2.1.0",
    "rollup-plugin-terser": "^4.0.4",
    "typescript": "^3.3.3333"
  }
}
