{
  "name": "web-client-boilerplate",
  "private": true,
  "version": "1.0.0",
  "description": "Web client boilerplate",
  "scripts": {
    "test": "ts-jest",
    "clean": "rimraf .cache dist",
    "build": "parcel build src/index.html --no-source-maps --no-cache",
    "dev": "parcel src/index.html",
    "start": "node server.js"
  },
  "browserslist": [
    "last 1 year and > 0.3% or last 2 firefox versions or unreleased versions"
  ],
  "alias": {
    "react-dom": "@hot-loader/react-dom"
  },
  "dependencies": {
    "classnames": "^2.2.6",
    "core-js": "^2.6.9",
    "express": "^4.17.1",
    "graphql": "^14.4.2",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-redux": "^7.1.0",
    "react-router": "^5.0.1",
    "react-router-dom": "^5.0.1",
    "redux": "^4.0.4",
    "reselect": "^4.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.5.5",
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@babel/plugin-proposal-object-rest-spread": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "@babel/preset-react": "^7.0.0",
    "@babel/preset-typescript": "^7.3.3",
    "@babel/register": "^7.5.5",
    "@hint/configuration-development": "^7.0.3",
    "@hot-loader/react-dom": "^16.8.6",
    "@types/express": "^4.17.0",
    "@types/jest": "^24.0.17",
    "@types/react": "^16.8.24",
    "@types/react-dom": "^16.8.4",
    "@types/react-redux": "^7.1.1",
    "@types/react-router": "^5.0.3",
    "@types/react-router-dom": "^4.3.4",
    "@typescript-eslint/eslint-plugin": "^1.12.0",
    "@typescript-eslint/parser": "^1.12.0",
    "autoprefixer": "^9.6.1",
    "eslint": "^5.16.0",
    "eslint-config-prettier": "^4.3.0",
    "eslint-plugin-compat": "^3.3.0",
    "eslint-plugin-graphql": "^3.0.3",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.14.2",
    "eslint-plugin-react-hooks": "^1.6.1",
    "hint": "^5.2.0",
    "jest": "^24.8.0",
    "parcel-bundler": "^1.12.3",
    "postcss-modules": "^1.4.1",
    "postcss-preset-env": "^6.7.0",
    "react-hot-loader": "^4.12.8",
    "rimraf": "^2.6.3",
    "sass": "^1.22.9",
    "typescript": "^3.5.3"
  }
