  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "tsc": "tsc --project tsconfig.json",
    "rollup:prod": "rollup --config rollup.config.js --environment INCLUDE_DEPS,BUILD:production",
    "rollup:dev": "npm run rollup:prod -- --environment INCLUDE_DEPS,BUILD:development",
    "rollup:watch": "npm run rollup:prod -- --environment INCLUDE_DEPS,BUILD:development --watch",
    "build:dev": "npm run tsc && npm run rollup:dev",
    "build:prod": "npm run tsc && npm run rollup:prod"
