{
  "name": "webapp-starter-kit",
  "version": "1.0.0",
  "description": "...",
  "main": "electron.js",
  "scripts": {
    "test": "ts-jest",
    "clean": "rimraf .cache dist",
    "build:full": "parcel build src/index.html --experimental-scope-hoisting --no-source-map --no-cache",
    "build": "parcel build src/index.html --no-cache",
    "dev": "cross-env NODE_ENV=development concurrently -n parcel,css 'parcel src/index.html --port 8888' 'tcm --camelCase --watch src/styles/'",
    "dev:electron": "cross-env NODE_ENV=development electron --inspect .",
    "serve": "node server.js",
    "start": "electron ."
  },
  "browserslist": [
    "last 1 year and > 0.3% or last 2 firefox versions or unreleased versions"
  ],
  "alias": {
    "react-dom": "@hot-loader/react-dom"
  },
  "dependencies": {
    "@babel/core": "^7.5.5",
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@babel/plugin-proposal-object-rest-spread": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "@babel/preset-react": "^7.0.0",
    "@babel/preset-typescript": "^7.3.3",
    "@babel/register": "^7.5.5",
    "@emotion/core": "^10.0.14",
    "@emotion/styled": "^10.0.14",
    "@hot-loader/react-dom": "^16.8.6",
    "core-js": "^2.6.9",
    "express": "^4.17.1",
    "graphql": "^14.4.2",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-redux": "^7.1.0",
    "redux": "^4.0.4",
    "reselect": "^4.0.0",
    "wouter": "^2.1.1"
  },
  "devDependencies": {
    "@types/express": "^4.17.0",
    "@types/jest": "^24.0.15",
    "@types/react": "^16.8.23",
    "@types/react-dom": "^16.8.4",
    "@types/react-redux": "^7.1.1",
    "@types/wouter": "^2.0.1",
    "@typescript-eslint/eslint-plugin": "^1.12.0",
    "@typescript-eslint/parser": "^1.12.0",
    "babel-plugin-emotion": "^10.0.14",
    "concurrently": "^4.1.1",
    "cross-env": "^5.2.0",
    "electron": "^5.0.8",
    "eslint": "^5.16.0",
    "eslint-config-prettier": "^4.3.0",
    "eslint-plugin-compat": "^3.3.0",
    "eslint-plugin-graphql": "^3.0.3",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.14.2",
    "eslint-plugin-react-hooks": "^1.6.1",
    "jest": "^24.8.0",
    "parcel-bundler": "^1.12.3",
    "postcss-modules": "^1.4.1",
    "postcss-preset-env": "^6.7.0",
    "react-hot-loader": "^4.12.8",
    "rimraf": "^2.6.3",
    "typed-css-modules": "^0.6.0",
    "typescript": "^3.5.3"
  }
}
