{
  "name": "web-client-boilerplate",
  "private": true,
  "version": "1.0.0",
  "description": "Web client boilerplate",
  "scripts": {
    "clean": "rimraf .cache dist",
    "dev": "parcel src/index.html",
    "test": "ts-jest",
    "build": "parcel build src/index.html --no-source-maps --no-cache --log-level 4 --detailed-report 30"
  },
  "browserslist": [
    "last 3 years and > 0.2%",
    "> 0.5%",
    "Firefox ESR",
    "not dead",
    "not IE <= 11",
    "not OperaMini all"
  ],
  "alias": {
    "react": "preact/compat",
    "react-dom": "preact/compat"
  },
  "dependencies": {
    "@uifabric/fluent-theme": "^7.1.1",
    "@uifabric/styling": "^7.5.0",
    "classnames": "^2.2.6",
    "graphql": "^14.4.2",
    "office-ui-fabric-react": "^7.25.0",
    "preact": "^10.0.0-rc.1",
    "react-redux": "^7.1.0",
    "redux": "^4.0.4",
    "use-subscription": "^1.0.0",
    "wouter": "^2.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "@types/classnames": "^2.2.9",
    "@types/jest": "^24.0.17",
    "@types/react": "^16.9.2",
    "@types/react-dom": "^16.8.5",
    "@typescript-eslint/eslint-plugin": "^2.0.0",
    "@typescript-eslint/parser": "^2.0.0",
    "autoprefixer": "^9.6.1",
    "core-js": "^2.6.9",
    "eslint": "^6.1.0",
    "eslint-config-prettier": "^6.0.0",
    "eslint-plugin-compat": "^3.3.0",
    "eslint-plugin-graphql": "^3.0.3",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.14.2",
    "eslint-plugin-react-hooks": "^1.7.0",
    "jest": "^24.9.0",
    "parcel-bundler": "^1.12.3",
    "postcss-modules": "^1.4.1",
    "postcss-preset-env": "^6.7.0",
    "prettier": "^1.18.2",
    "rimraf": "^3.0.0",
    "sass": "^1.22.10",
    "stylelint": "^10.1.0",
    "stylelint-config-prettier": "^5.2.0",
    "stylelint-config-recommended-scss": "^3.3.0",
    "stylelint-config-standard": "^18.3.0",
    "stylelint-prettier": "^1.1.1",
    "stylelint-scss": "^3.9.3",
    "typescript": "^3.5.3"
  }
}
